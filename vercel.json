{
  "version": 2,
  "builds": [
      {
          "src":"./index.js",
          "use":"@vercel/node"
      }
  ],
  "routes":[
      {
          "src":"/(.*)",
          "dest":"/"
      }
  ],
  "redirects": [
      {
        "source": "/(.*)/?",
        "destination": "https://www.gaziantepkombiservis.com/",
        "type": 301
      }
    ]
  
}
