{
  "version": 2,
  "builds": [
      {
          "src":"./index.js",
          "use":"@vercel/node"
      }
  ],
  "routes":[
      {
          "src":"/(.*)",
          "dest":"/"
      }
  ],
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://yourdomainhere.com/:1",
      "statusCode": 301
    }
  ]
}
