{
  "version": 2,
  "builds": [
      {
          "src":"./index.js",
          "use":"@vercel/node"
      }
  ],
  "routes":[
      {
          "src":"/(.*)",
          "dest":"/"
      }
  ],
  "rewrites": [
    {
      "source": "http://www.gaziantepkombiservis.com/",
      "destination": "https://www.gaziantepkombiservis.com/:1",
    }
  ]
}
